        "/api/v1/business-data/create": {
            "post": {
                "tags": [
                    "BusinessData"
                ],
                "summary": "Create Business Data ",
                "description": "Create Business Data",
                "operationId": "createBusinessData",
                "requestBody": {
                    "description": "Create a Business Data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "business_name",
                                    "address",
                                    "experience"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "your email",
                                        "type": "string",
                                        "example": "user@example.com"
                                    },
                                    "business_name": {
                                        "description": "your Business Name",
                                        "type": "string",
                                        "example": "trapzium"
                                    },
                                    "address": {
                                        "description": "your business address",
                                        "type": "string",
                                        "example": "Dansoman Roundabout, Accra"
                                    },
                                    "experience": {
                                        "description": "Experience",
                                        "type": "string",
                                        "example": "Food & nightlife, sports events, talks & workshop"
                                    },
                                    "location": {
                                        "properties": {
                                            "street_number": {
                                                "type": "string",
                                                "example": "213"
                                            },
                                            "route": {
                                                "type": "string",
                                                "example": "Hamburgo"
                                            },
                                            "political": {
                                                "type": "string",
                                                "example": "Juárez"
                                            },
                                            "administrative_area_level_1": {
                                                "type": "string",
                                                "example": "Hamburgo"
                                            },
                                            "country": {
                                                "type": "string",
                                                "example": "Mexico"
                                            },
                                            "postal_code": {
                                                "type": "string",
                                                "example": "06600"
                                            },
                                            "lat": {
                                                "type": "string",
                                                "example": "19.4245"
                                            },
                                            "lon": {
                                                "type": "string",
                                                "example": "-99.1688"
                                            },
                                            "address": {
                                                "type": "string",
                                                "example": "Hamburgo 213, Juárez, Mexico City, CDMX, Mexico"
                                            },
                                            "city": {
                                                "type": "string",
                                                "example": "Ciudad de México"
                                            },
                                            "is_active": {
                                                "type": "string",
                                                "example": "true"
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "1"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Record Added Successfully."
                                        },
                                        "business_data": {
                                            "$ref": "#/components/schemas/BusinessData"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
